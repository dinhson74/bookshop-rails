<tr>
  <td scope='row'><%= user.name%></td>
  <td scope='row'><%= user.email%></td>
  <td scope='row'><%= user.address%></td>
  <td scope='row'><%= user.phone%></td>
  <td scope='row'><%= user.birthday%></td>
  <td scope='row'><%= user.admin%></td>
  <td scope='row'>
    <%= link_to I18n.t('common.update'), edit_admin_user_path(user), class: 'btn btn-primary' %>
    <%= link_to I18n.t('common.destroy'), admin_user_path(user), class: 'btn btn-primary',
       method: :delete, data: {confirm: I18n.t('common.are_you_sure') } %>
  </td>
</tr>
